    "user = input(\"Enter :\")\n",
    "l = len(user)\n",
    "new = 0\n",
    "for i in user:\n",
    "    v = int(i)**l\n",
    "    new = new + v\n",
    "\n",
    "if int(user) == new:\n",
    "    print(\"True\")\n",
    "else:\n",
    "    print(\"False\")"
